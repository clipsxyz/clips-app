{
  "name": "gazetteer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "dev:ios": "react-native run-ios",
    "dev:android": "react-native run-android",
    "dev:clean": "npm run clean:cache && vite",
    "build": "vite build",
    "build:web": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "fetch-music": "node scripts/fetchRoyaltyFreeMusic.js",
    "clean:cache": "node -e \"const fs = require('fs'); const path = require('path'); const rimraf = (dir) => { try { fs.rmSync(dir, { recursive: true, force: true }); console.log('Cleared:', dir); } catch(e) {} }; rimraf(path.join(process.cwd(), 'node_modules/.vite')); rimraf(path.join(process.cwd(), 'dist')); console.log('Cache cleared!');\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@react-navigation/bottom-tabs": "^7.8.2",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@sentry/react": "^10.36.0",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.7.0",
    "@types/qrcode": "^1.5.6",
    "firebase": "^10.7.1",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "idb-keyval": "^6.2.1",
    "nativewind": "^4.2.1",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^4.12.0",
    "react-native": "^0.84.0",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-native-webrtc": "^124.0.7",
    "react-router-dom": "^6.8.1",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.26.3",
    "tone": "^15.1.22",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@size-limit/preset-app": "^12.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-basic-ssl": "^2.1.3",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.27.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "size-limit": "^10.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^7.3.1",
    "vitest": "^0.34.0"
  },
  "size-limit": [
    {
      "name": "entry js",
      "path": "dist/assets/*-legacy*.js",
      "limit": "150 KB"
    },
    {
      "name": "entry js",
      "path": "dist/assets/*-[a-f0-9].js",
      "limit": "150 KB"
    },
    {
      "name": "async chunks",
      "path": "dist/assets/chunk-*.js",
      "limit": "200 KB"
    },
    {
      "name": "css",
      "path": "dist/assets/*.css",
      "limit": "30 KB"
    }
  ]
}
